[ExtensionOf(tableStr(InventJournalTrans))]
final class InventJournalTrans_Extension
{
    public void modifiedField(FieldId _fieldId)
    {
        next modifiedField(_fieldId);

        switch(_fieldId)
        {
            case fieldNum(InventJournalTrans, Vendor):
            if(this.Vendor)
